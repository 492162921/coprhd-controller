<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="cinderapifactory" class="com.emc.storageos.cinder.api.CinderApiFactory"
        init-method="init" >
    </bean>

    <bean id="cinder"
        class="com.emc.storageos.volumecontroller.impl.plugins.CinderCommunicationInterface">
        <property name="CinderApiFactory" ref="cinderapifactory" />
    </bean>

    <bean id="openstack"
        class="com.emc.storageos.volumecontroller.impl.plugins.CinderCommunicationInterface">
        <property name="CinderApiFactory" ref="cinderapifactory" />
    </bean>

</beans>
