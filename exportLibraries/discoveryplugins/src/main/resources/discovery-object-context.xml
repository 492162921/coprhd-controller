<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="objectapifactory" class="com.emc.storageos.object.api.ObjectApiFactory"
        init-method="init" >
    </bean>

    <bean id="object"
        class="com.emc.storageos.volumecontroller.impl.plugins.ObjectCommunicationInterface">
        <property name="ObjectApiFactory" ref="objectapifactory" />
    </bean>
</beans>