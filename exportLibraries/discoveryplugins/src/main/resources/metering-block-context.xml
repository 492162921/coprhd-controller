<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- List of Namespaces -->
	<bean id="namespaces"
		class="com.emc.storageos.plugins.common.domainmodel.NamespaceList">
		<property name="nsList">
			<map>
				<entry key="metering" value-ref="collectMetering" />
            </map>
		</property>
	</bean>
	<!-- List of operations for this performance namespace -->
	<bean id="collectMetering"
		class="com.emc.storageos.plugins.common.domainmodel.Namespace">
		<property name="_operations">
			<list>
				<ref bean="system" />
				<ref bean="blockStatisticsService" />
				<ref bean="blockStatisticsCapabilities" />
				<ref bean="queryStatisticsCollection" />
				<!-- beans to get statistics for Volume -->
				<ref bean="checkIfManifestCollectionExists" />
				<ref bean="createManifestCollection" />
				<ref bean="addVolumeManifest" />
				<ref bean="getVolumeMetricSequence" />
				<ref bean="getVolumeStatistics" />
				
				
				<!-- beans to get Statistics for Array -->
				<ref bean="addArrayManifest" />
				<ref bean="getArrayMetricsSequence" />
				<ref bean="getArrayStatistics" />

				<!-- beans to get Statistics for FEAdapt; do this before FEPort -->
				<ref bean="addFEAdaptManifest" />
				<ref bean="getFEAdaptMetricsSequence" />
				<ref bean="getFEAdaptStatistics" />
				
				<!-- beans to get Statistics for FEPort -->
				<ref bean="addFEPortManifest" />
				<ref bean="getFEPortMetricsSequence" />
				<ref bean="getFEPortStatistics" />
				
				<ref bean="getStoragePools" />
				<ref bean="getVolumeCapacity" />
				<ref bean="getVolumeCapacityFor8x"/>
				
			</list>
		</property>
	</bean>

	<!-- SMI Call to get CIMObject Path for the System -->
	<bean id="system"
		class="com.emc.storageos.plugins.common.domainmodel.Operation">
		<property name="_arguments">
			<list>
				<ref bean="reference-comp" />
			</list>
		</property>
		<property name="instance" ref="cimClient" />
		<property name="_result" value="ComputerSystem" />
		<property name="_method" value="enumerateInstanceNames" />
		<property name="_processor" ref="cimpathprocessor" />
        <property name="message" value="Invoking EnumerateInstanceNames to get Managed Array"/>
	</bean>


	<!-- SMI Call to get Block Statistics Service Instance from Provider -->
	<bean id="blockStatisticsService"
		class="com.emc.storageos.plugins.common.domainmodel.Operation">
		<property name="_arguments">
			<list>
				<ref bean="reference-comp" />
				<ref bean="null" />
				<ref bean="resultClass-cimblock" />
				<ref bean="null" />
				<ref bean="null" />
			</list>
		</property>
		<property name="instance" ref="cimClient" />
		<property name="_result" value="BlockStatisticsService" />
		<property name="_method" value="associatorNames" />
		<property name="_processor" ref="cimpathprocessor" />
		<property name="message" value="Invoking associatorNames to get BlockStatistics Service"/>
	</bean>

	<!-- SMI Call to get Block Statistics Capabilities Instance -->
	<bean id="blockStatisticsCapabilities"
		class="com.emc.storageos.plugins.common.domainmodel.Operation">
		<property name="_arguments">
			<list>
				<ref bean="reference-block" />
				<ref bean="null" />
				<ref bean="resultClass-capabilities" />
				<ref bean="null" />
				<ref bean="null" />
				<ref bean="bool-true" />
				<ref bean="null" />
			</list>
		</property>
		<property name="instance" ref="cimClient" />
		<property name="_result" value="BlockStatisticsCapabilities" />
		<property name="_method" value="associatorInstances" />
		<property name="_processor" ref="blockStatisticsCapabilitiesProcessor" />
		<property name="message" value="Invoking associatorInstances to get BlockStatisticsCapabilities"/>
	</bean>
	
	<!-- SMI Call to check whether manifestCollection exists or not.-->
	<bean id="checkIfManifestCollectionExists"
		class="com.emc.storageos.plugins.common.domainmodel.Operation">
		<property name="_arguments">
			<list>
			    <ref bean="reference-comp" />
			    <ref bean="null" />
				<ref bean="resultManifestCollectionClass-query" />
				<ref bean="null" />
				<ref bean="null" />
				<ref bean="bool-true" />
				<ref bean="elementNameProp" />
			</list>
		</property>
		<property name="instance" ref="cimClient" />
		<property name="_method" value="associatorInstances" />
		<property name="_result" value="manifestCollection" />
		<property name="_processor" ref="checkManifestExistsProcessor" />
		<property name="message" value="Invoking associatorInstances to checkIfManifestCollectionExists"/>
	</bean>
	
	<!-- SMI Call to get Query Statistics Collection Instance from Provider -->
	<bean id="queryStatisticsCollection"
		class="com.emc.storageos.plugins.common.domainmodel.Operation">
		<property name="_arguments">
			<list>
				<ref bean="reference-comp" />
				<!-- I didn't use the inbuilt <null/> as I need everything to be a Argument 
					Object -->
				<ref bean="null" />
				<ref bean="resultClass-query" />
				<ref bean="null" />
				<ref bean="null" />
				<ref bean="bool-true" />
				<ref bean="timeSampledprop" />
			</list>
		</property>
		<property name="instance" ref="cimClient" />
		<property name="_result" value="QueryStatistics" />
		<property name="_method" value="associatorInstances" />
		<property name="_processor" ref="queryStatisticsProcessor" />
		<property name="message" value="Invoking associatorInstances to get queryStatisticsCollection"/>
	</bean>
	<!-- SMI Call to create Manifest Collection on Provider for Volume stats -->
	<bean id="createManifestCollection"
		class="com.emc.storageos.plugins.common.domainmodel.Operation">
		<property name="_arguments">
			<list>
				<ref bean="reference-block" />
				<ref bean="intrinsicMethodName-manifestcollection" />
				<ref bean="cimArg-manifestCollection" />
				<ref bean="cimoutputArray" />
			</list>
		</property>
		<property name="instance" ref="cimClient" />
		<property name="_result" value="manifestCollection" />
		<property name="_method" value="invokeMethod" />
		<property name="executionCycles" value="manifestExists"/>
		<property name="_processor" ref="CIMArgumentArrayProcessor" />
		<property name="message" value="Invoking associatorInstances to get createManifestCollection"/>
	</bean>
	<!-- SMI Call to add Volume Manifest to already created ManifestCollection 
		on Provider -->
	<bean id="addVolumeManifest"
		class="com.emc.storageos.plugins.common.domainmodel.Operation">
		<property name="_arguments">
			<list>
				<ref bean="reference-block" />
				<ref bean="intrinsicMethodName-addmanifest" />
				<ref bean="cimArgVolume-manifest" />
				<ref bean="cimoutputArray" />
			</list>
		</property>
		<property name="instance" ref="cimClient" />
		<property name="_result" value="volumemanifest" />
		<property name="_method" value="invokeMethod" />
		<property name="_processor" ref="CIMArgumentArrayProcessor" />
		<property name="message" value="Invoking associatorInstances to addVolumeManifest"/>
	</bean>
	
	<!-- SMI Call to get the sequence of Volume metrics.-->
	<bean id="getVolumeMetricSequence"
		class="com.emc.storageos.plugins.common.domainmodel.Operation">
		<property name="_arguments">
			<list>
			    <ref bean="manifestCollection-ref" />
			    <ref bean="assoc_memberCollection" />
				<ref bean="resultBlockStatisticsManifest" />
				<ref bean="null" />
				<ref bean="null" />
				<ref bean="bool-true" />
				<ref bean="null" />
			</list>
		</property>
		<property name="instance" ref="cimClient" />
		<property name="_method" value="associatorInstances" />
		<property name="_result" value="" />
		<property name="_processor" ref="metricSequenceProcessor" />
		<property name="message" value="Invoking associatorInstances to getVolumeMetricSequence"/>
	</bean>
	

	<!-- SMI Call to add Array Manifest to already created ManifestCollection 
		on Provider -->
	<bean id="addArrayManifest"
		class="com.emc.storageos.plugins.common.domainmodel.Operation">
		<property name="_arguments">
			<list>
				<ref bean="reference-block" />
				<ref bean="intrinsicMethodName-addmanifest" />
				<ref bean="cimArgArray-addmanifest" />
				<ref bean="cimoutputArray" />
			</list>
		</property>
		<property name="instance" ref="cimClient" />
		<property name="_result" value="arraymanifest" />
		<property name="_method" value="invokeMethod" />
		<property name="_processor" ref="CIMArgumentArrayProcessor" />
		<property name="message" value="Invoking Add Array metrics Manifest into Collection"/>
	</bean>
	
		<!-- SMI Call to get the sequence of FEPort metrics.-->
	<bean id="getArrayMetricsSequence"
		class="com.emc.storageos.plugins.common.domainmodel.Operation">
		<property name="_arguments">
			<list>
			    <ref bean="manifestCollection-ref" />
			    <ref bean="assoc_memberCollection" />
				<ref bean="resultBlockStatisticsManifest" />
				<ref bean="null" />
				<ref bean="null" />
				<ref bean="bool-true" />
				<ref bean="null" />
			</list>
		</property>
		<property name="instance" ref="cimClient" />
		<property name="_method" value="associatorInstances" />
		<property name="_result" value="" />
		<property name="_processor" ref="metricSequenceProcessor" />
		<property name="message" value="Invoking associatorInstances to get ArrayMetricsSequence"/>
	</bean>
	
	<!-- SMI Call to add FEPort Manifest to already created ManifestCollection 
		on Provider -->
	<bean id="addFEPortManifest"
		class="com.emc.storageos.plugins.common.domainmodel.Operation">
		<property name="_arguments">
			<list>
				<ref bean="reference-block" />
				<ref bean="intrinsicMethodName-addmanifest" />
				<ref bean="cimArgFEPort-addmanifest" />
				<ref bean="cimoutputArray" />
			</list>
		</property>
		<property name="instance" ref="cimClient" />
		<property name="_result" value="feportmanifest" />
		<property name="_method" value="invokeMethod" />
		<property name="_processor" ref="CIMArgumentArrayProcessor" />
		<property name="message" value="Invoking Add FEPort metrics Manifest into Collection"/>
	</bean>

	<!-- SMI Call to add FEAdapt Manifest to already created ManifestCollection 
		on Provider -->
	<bean id="addFEAdaptManifest"
		class="com.emc.storageos.plugins.common.domainmodel.Operation">
		<property name="_arguments">
			<list>
				<ref bean="reference-block" />
				<ref bean="intrinsicMethodName-addmanifest" />
				<ref bean="cimArgFEAdapt-addmanifest" />
				<ref bean="cimoutputArray" />
			</list>
		</property>
		<property name="instance" ref="cimClient" />
		<property name="_result" value="feadaptmanifest" />
		<property name="_method" value="invokeMethod" />
		<property name="_processor" ref="CIMArgumentArrayProcessor" />
		<property name="message" value="Invoking Add FEAdapt metrics Manifest into Collection"/>
	</bean>
	
	<!-- SMI Call to get the sequence of FEPort metrics.-->
	<bean id="getFEPortMetricsSequence"
		class="com.emc.storageos.plugins.common.domainmodel.Operation">
		<property name="_arguments">
			<list>
			    <ref bean="manifestCollection-ref" />
			    <ref bean="assoc_memberCollection" />
				<ref bean="resultBlockStatisticsManifest" />
				<ref bean="null" />
				<ref bean="null" />
				<ref bean="bool-true" />
				<ref bean="null" />
			</list>
		</property>
		<property name="instance" ref="cimClient" />
		<property name="_method" value="associatorInstances" />
		<property name="_result" value="" />
		<property name="_processor" ref="metricSequenceProcessor" />
		<property name="message" value="Invoking associatorInstances to getFEPortMetricsSequence"/>
	</bean>

	<!-- SMI Call to get the sequence of FEAdapt metrics.-->
	<bean id="getFEAdaptMetricsSequence"
		class="com.emc.storageos.plugins.common.domainmodel.Operation">
		<property name="_arguments">
			<list>
			    <ref bean="manifestCollection-ref" />
			    <ref bean="assoc_memberCollection" />
				<ref bean="resultBlockStatisticsManifest" />
				<ref bean="null" />
				<ref bean="null" />
				<ref bean="bool-true" />
				<ref bean="null" />
			</list>
		</property>
		<property name="instance" ref="cimClient" />
		<property name="_method" value="associatorInstances" />
		<property name="_result" value="" />
		<property name="_processor" ref="metricSequenceProcessor" />
		<property name="message" value="Invoking associatorInstances to getFEAdaptMetricsSequence"/>
	</bean>
	
	
	<!-- SMI Call to retrieve Volume Statistics -->
	<bean id="getVolumeStatistics"
		class="com.emc.storageos.plugins.common.domainmodel.Operation">
		<property name="_arguments">
			<list>
				<ref bean="reference-block" />
				<ref bean="statisticsCollection" />
				<ref bean="cimArgVolume-statistics" />
				<ref bean="cimoutputArray" />
			</list>
		</property>
		<property name="instance" ref="cimClient" />
		<property name="_result" value="" />
		<property name="_method" value="invokeMethod" />
		<property name="_processor" ref="volumeProcessor" />
		<property name="message" value="Invoking getVolumeStatistics"/>
	</bean>
	
	<!-- SMI Call to retrieve Array Statistics -->
	<bean id="getArrayStatistics"
		class="com.emc.storageos.plugins.common.domainmodel.Operation">
		<property name="_arguments">
			<list>
				<ref bean="reference-block" />
				<ref bean="statisticsCollection" />
				<ref bean="cimArgArray-statistics" />
				<ref bean="cimoutputArray" />
			</list>
		</property>
		<property name="instance" ref="cimClient" />
		<property name="_result" value="" />
		<property name="_method" value="invokeMethod" />
		<property name="_processor" ref="arrayStatsProcessor" />
		<property name="message" value="Invoking getArrayStatistics"/>
	</bean>
	
	<!-- SMI Call to retrieve FEPort Statistics -->
	<bean id="getFEPortStatistics"
		class="com.emc.storageos.plugins.common.domainmodel.Operation">
		<property name="_arguments">
			<list>
				<ref bean="reference-block" />
				<ref bean="statisticsCollection" />
				<ref bean="cimArgFEPort-statistics" />
				<ref bean="cimoutputArray" />
			</list>
		</property>
		<property name="instance" ref="cimClient" />
		<property name="_result" value="" />
		<property name="_method" value="invokeMethod" />
		<property name="_processor" ref="fePortStatsProcessor" />
		<property name="message" value="Invoking getFEPortStatistics"/>
	</bean>

	<!-- SMI Call to retrieve FEAdapt Statistics -->
	<bean id="getFEAdaptStatistics"
		class="com.emc.storageos.plugins.common.domainmodel.Operation">
		<property name="_arguments">
			<list>
				<ref bean="reference-block" />
				<ref bean="statisticsCollection" />
				<ref bean="cimArgFEAdapt-statistics" />
				<ref bean="cimoutputArray" />
			</list>
		</property>
		<property name="instance" ref="cimClient" />
		<property name="_result" value="" />
		<property name="_method" value="invokeMethod" />
		<property name="_processor" ref="feAdaptStatsProcessor" />
		<property name="message" value="Invoking getFEAdaptStatistics"/>
	</bean>

	<!-- SMI Call to calculate Snapshot details for Volumes -->
	<bean id="storagesynchronized"
		class="com.emc.storageos.plugins.common.domainmodel.Operation">
		<property name="_arguments">
			<list>
				<ref bean="reference-synchronized" />
				<ref bean="bool-true" />
				<ref bean="bool" />
				<ref bean="ssn-propList" />
				<ref bean="null" />
			    <ref bean="null" />
			    <ref bean="timeout" />
			    <ref bean="bool" />
			    <ref bean="maxObjects" />
			</list>
		</property>
		<property name="instance" ref="cimClient" />
		<property name="_result" value="stsynchronized" />
		<property name="_method" value="enumerateInstances" />
		<property name="_processor" ref="storagesynchronizedprocessor" />
        <property name="message" value="Invoking enumerateInstances to get Replica Information"/>
	</bean>
	
	<bean id="maxObjects" class="com.emc.storageos.plugins.common.domainmodel.Argument">
        <property name="_creator" ref="argscreator" />
        <property name="_method" value="getIntValue32" />
        <property name="_value" value="10" />
        <property name="_name" value="maxObjects" />
    </bean>
    
    <bean id="timeout" class="com.emc.storageos.plugins.common.domainmodel.Argument">
        <property name="_creator" ref="argscreator" />
        <property name="_method" value="getIntValue32" />
        <property name="_value" value="1200" />
        <property name="_name" value="timeoutinSeconds" />
    </bean>

	<!-- SMI Call to get StoragePools manged by that Array -->
	<bean id="getStoragePools"
		class="com.emc.storageos.plugins.common.domainmodel.Operation">
		<property name="_arguments">
			<list>
				<ref bean="reference-storagePool" />

			</list>
		</property>
		<property name="instance" ref="cimClient" />
		<property name="_result" value="storagePools" />
		<property name="_method" value="enumerateInstanceNames" />
		<property name="_processor" ref="storagepoolprocessor" />
        <property name="message" value="Invoking enumerateInstanceNames to get Storage Pools"/>
	</bean>


	<bean id="getVolumeCapacity"
		class="com.emc.storageos.plugins.common.domainmodel.Operation">
		<property name="_arguments">
			<list>
				<ref bean="reference-storagePools" />
				<ref bean="null" />
				<ref bean="resultClass-volumeview" />
				<ref bean="null" />
				<ref bean="null" />
				<ref bean="bool-true" />
				<ref bean="volumeview-prop" />
				<ref bean="null" />
                <ref bean="null" />
                <ref bean="timeout" />
                <ref bean="bool" />
                <ref bean="maxObjects" />
			</list>
		</property>
		<property name="instance" ref="cimClient" />
		<property name="_result" value="" />
		<property name="_method" value="associators" />
		<property name="_processor" ref="capacitypoolprocessor" />
		<property name="message" value="Invoking associatorInstances to get Capacity information for Volumes"/>
		<property name="supportedVersion" value="4.x"/>
	</bean>
	
	<!-- SMI Call to get volume capacity information using provider 8.x -->
    <bean id="getVolumeCapacityFor8x" class="com.emc.storageos.plugins.common.domainmodel.Operation">
        <property name="_arguments">
            <list>
                <ref bean="reference-storagePools" />
                <ref bean="assocClass-CIM_AllocatedFromStoragePool" />
                <ref bean="resultClass-Volume" />
                <ref bean="null" />
                <ref bean="null" />
                <ref bean="bool-true" />
                <ref bean="volumeview-prop8x" />
                <ref bean="null" />
                <ref bean="null" />
                <ref bean="timeout" />
                <ref bean="bool" />
                <ref bean="maxObjects" />
            </list>
        </property>
        <property name="instance" ref="cimClient" />
        <property name="_result" value="" />
        <property name="_method" value="associators" />
        <property name="_processor" ref="capacitypoolprocessor" />
        <property name="supportedVersion" value="8.x"/>
        <property name="message" value="Invoking associatorInstances to get Capacity information for volumes using provider version 8.x"/>
    </bean>
    
    <bean id="resultClass-Volume" class="com.emc.storageos.plugins.common.domainmodel.Argument">
        <property name="_creator" ref="argscreator" />
        <property name="_method" value="getStringValue" />
        <property name="_value" value="CIM_StorageVolume" />
    </bean>
    
    <bean id="assocClass-CIM_AllocatedFromStoragePool" class="com.emc.storageos.plugins.common.domainmodel.Argument">
        <property name="_creator" ref="argscreator" />
        <property name="_method" value="getStringValue" />
        <property name="_value" value="CIM_AllocatedFromStoragePool" />
    </bean>
    
    <bean id="volumeview-prop8x" class="com.emc.storageos.plugins.common.domainmodel.Argument">
        <property name="_creator" ref="argscreator" />
        <property name="_method" value="getStringArrayValue" />
        <property name="_value"
            value="EMCSpaceConsumed,BlockSize,ConsumableBlocks,SystemName,DeviceID" />
        <property name="_name" value="prvcList" />
    </bean>

	
	<!-- cimClient -->
	 <bean id="cimClient"
		class="com.emc.storageos.plugins.common.domainmodel.Argument">
		<property name="_creator" ref="argscreator" />
		<property name="_method" value="getReferenceValue" />
		<property name="_value" value="cimClient" />
	</bean>


	<!-- Processors -->
	<bean id="cimpathprocessor"
		class="com.emc.storageos.plugins.common.processor.CIMPathProcessor" />
	<bean id="queryStatisticsProcessor"
		class="com.emc.storageos.volumecontroller.impl.plugins.metering.smis.processor.QueryStatisticsProcessor" />
	<bean id="CIMArgumentArrayProcessor"
		class="com.emc.storageos.plugins.common.processor.CIMArgumentArrayProcessor" />

        <bean id="blockStatisticsCapabilitiesProcessor"
	    class="com.emc.storageos.volumecontroller.impl.plugins.metering.smis.processor.BlockStatisticsCapabilitiesProcessor" />
		
	<bean id="checkManifestExistsProcessor"
	      class="com.emc.storageos.volumecontroller.impl.plugins.metering.smis.processor.CheckManifestExistsProcessor" />
	      
	<bean id="volumeProcessor"
		class="com.emc.storageos.volumecontroller.impl.plugins.metering.smis.processor.VolumeProcessor">
		<property name="zeroRecordGenerator" ref="zeroRecordGenerator" />
		<property name="statsColumnInjector" ref="statsColumnInjector" />
	</bean>
	
	<bean id="arrayStatsProcessor"
		class="com.emc.storageos.volumecontroller.impl.plugins.metering.smis.processor.StorageSystemStatsProcessor">
		<property name="zeroRecordGenerator" ref="zeroRecordGenerator" />
	</bean>
	
	<bean id="fePortStatsProcessor"
		class="com.emc.storageos.volumecontroller.impl.plugins.metering.smis.processor.FEPortStatsProcessor">
		<property name="zeroRecordGenerator" ref="zeroRecordGenerator" />
		<property name="portMetricsProcessor" ref="portMetricsProcessor" />
	</bean>	

	<bean id="feAdaptStatsProcessor"
		class="com.emc.storageos.volumecontroller.impl.plugins.metering.smis.processor.FEAdaptStatsProcessor">
		<property name="zeroRecordGenerator" ref="zeroRecordGenerator" />
		<property name="portMetricsProcessor" ref="portMetricsProcessor" />
	</bean>	
	
	<bean id="metricSequenceProcessor"
	     class="com.emc.storageos.volumecontroller.impl.plugins.metering.smis.processor.MetricSequenceProcessor">
	</bean>

	<bean id="zeroRecordGenerator"
		class="com.emc.storageos.volumecontroller.impl.plugins.metering.smis.BlockZeroRecordGenerator" />

	<bean id="capacitypoolprocessor"
		class="com.emc.storageos.volumecontroller.impl.plugins.metering.smis.processor.CapacityPoolProcessor" />

	<bean id="storagesynchronizedprocessor"
		class="com.emc.storageos.volumecontroller.impl.plugins.metering.smis.processor.SSNProcessor" />

	<!-- Processors -->
	<bean id="storagepoolprocessor"
		class="com.emc.storageos.volumecontroller.impl.plugins.metering.smis.processor.StoragePoolProcessor" />

	<!-- ArgsCreator Instance -->
	<bean id="argscreator" class="com.emc.storageos.plugins.common.ArgsCreator">
		<constructor-arg ref="smiutility" />
	</bean>
	<!-- SMIUtility -->
	<bean id="smiutility" class="com.emc.storageos.plugins.common.Util" />


	<!-- Arguments -->

	<bean id="null"
		class="com.emc.storageos.plugins.common.domainmodel.Argument">
		<property name="_creator" ref="argscreator" />
		<property name="_method" value="getStringValue" />
		<property name="_value">
			<null />
		</property>
	</bean>

	<bean id="resultClass-cimblock"
		class="com.emc.storageos.plugins.common.domainmodel.Argument">
		<property name="_creator" ref="argscreator" />
		<property name="_method" value="getStringValue" />
		<property name="_value" value="CIM_BlockStatisticsService" />
	</bean>
	<bean id="resultClass-capabilities"
		class="com.emc.storageos.plugins.common.domainmodel.Argument">
		<property name="_creator" ref="argscreator" />
		<property name="_method" value="getStringValue" />
		<property name="_value" value="CIM_BlockStatisticsCapabilities" />
