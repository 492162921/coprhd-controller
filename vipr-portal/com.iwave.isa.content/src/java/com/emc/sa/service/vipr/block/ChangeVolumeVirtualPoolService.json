      lockable: true
    },  
    
    volume:{
      type:"assetType.vipr.sourceBlockVolume"
    },
    
    virtualPoolChangeOperation:{
      type:"assetType.vipr.virtualPoolChangeOperation",
      lockable: true
    },
   	
    targetVirtualPool:{
      type:"assetType.vipr.targetVirtualPool"
    },
    
    rdfGroup:{
      type:"assetType.vipr.rdfGroupChangeVpool",
      required: false,
      hideIfEmpty: true
    },

    consistencyGroup: {
      type:"assetType.vipr.consistencyGroupWithVirtualPoolChangeOperation",
      required: false
    },
        
    exportPathPolicy: {
        type:"assetType.vipr.exportPathPolicies",
        required: false
    }
  }
  
}
