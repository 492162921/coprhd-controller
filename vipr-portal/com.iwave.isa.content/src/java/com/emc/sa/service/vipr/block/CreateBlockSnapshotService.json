{
  baseKey:"CreateBlockSnapshot.Block",
  serviceId:"CreateBlockSnapshot",
  fields:{
    project:{
      type:"assetType.vipr.project",
      lockable: true
    },

    storageType:{
      type:"assetType.vipr.blockVolumeOrConsistencyType",
      lockable:true,
      initialValue:"volume"
    },
    
    type: {
      type: "assetType.vipr.blockSnapshotType",
      initialValue: "local",
      required: false,
      lockable: true
    },
    
    volumes:{
      type:"assetType.vipr.snapshotBlockVolume"
    },

    readOnly: {
      type: "boolean",
      initialValue: "false",
      required: "false",
      lockable: true
    },
    
    name:{
      type:"text",
      validation:{
        min:2,
        max:128
      }
    },
    
    advanced:{
      type:"group",
      lockable: true,
      collapsed:true,
      items:{
        linkSnapshot:{
          type:"table",
          items:{
            name:{
              type:"text",
              validation:{
                min:2,
                max:128
              }
            },
            numberOfSnapshots:{
              type:"number",
              initialValue: "1",
              validation:{
                  min:1
              }
            }
          }
        }
      }
    }
  }
}
