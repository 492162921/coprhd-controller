{
  baseKey:"FailoverBlockVolume.Block",
  serviceId:"FailoverBlockVolume",
  destructive: true,
  fields: {
    project:{
      type:"assetType.vipr.project",
      lockable: true
    },
    
    storageType:{
      type:"assetType.vipr.blockVolumeOrConsistencyType",
      lockable:true,
      initialValue:"volume"
    },
	
    volumes:{
      type:"assetType.vipr.protectedBlockVolume"
    },

    failoverTarget:{
      type: "assetType.vipr.failoverTarget"
    },
    
    imageToAccess:{
    	type: "assetType.vipr.imageToAccess",
    	required: "false",
        lockable: true
    }, 
    
    pointInTime:{
      type:"table",
      hideTableBorder,
      hideHeader,
      hideAddButton,
      hideRemoveRow,
      items:{
        imageDate:{
          type:"failoverDate",
          required: "false"
        },
        imageTime:{
          type:"failoverDate",
          required: "false"
        }
      }
    }    
    
  }
}
