<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <!-- On deployed system, content of this file is generated from api-var-template.xml -->
    <bean id="serviceinfo" class="com.emc.storageos.coordinator.common.impl.ServiceImpl">
        <property name="name" value="geosvc"/>
        <property name="id" value="geosvc-standalone"/>
        <property name="endpoint" value="http://localhost:8180/"/>
        <property name="version" value="1"/>
    </bean>
    
    <bean id="nodeCount" class="java.lang.Integer">
        <constructor-arg type="java.lang.Integer" value="1" />
    </bean>
    
    <bean id="beacon" class="com.emc.storageos.coordinator.client.beacon.impl.ServiceBeaconImpl" init-method="init" destroy-method="stop">
        <property name="zkConnection" ref="zkconn"/>
        <property name="service" ref="serviceinfo"/>
        <property name="siteId" value="63bbac33-c68f-9afd-eb9c-10ac70375f0d"/>                            
    </bean>

</beans>
