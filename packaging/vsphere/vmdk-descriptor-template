# Disk DescriptorFile
version=1
CID=54f69138
parentCID=ffffffff
createType="monolithicFlat"

# Extent description
RW 2088 FLAT "${1}" 0

# The Disk Data Base
# DDB
ddb.virtualHWVersion = "4"
